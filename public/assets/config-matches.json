{
  "heading": "Charged Up - 2023",
  "logo": "logo.png",
  "pages": [
    {
      "name": "Setup+Autonomous",
      "widgets": [
        {
          "name": "Scouter Name",
          "type": "text",
          "colspan": 2
        },
        {
          "name": "Starting Position",
          "type": "positions",
          "file": "field.png",
          "colspan": 2
        },
        {
          "name": "Left Community",
          "type": "checkbox"
        },
        {
          "name": "Grid",
          "type": "positions",
          "file": "grid.png",
          "colspan": 2,
          "allowMultiple": true,
          "selectColor": "black",
          "prefix": "Auto"
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Charging Station",
          "type": "dropdown",
          "options": [
            "Not Attempted",
            "Engaged",
            "Docked",
            "Failed"
          ]
        }
      ]
    },
    {
      "name": "Teleop",
      "widgets": [
        {
          "name": "Picked Up From",
          "type": "heading",
          "colspan": 5,
          "align": "center"
        },
        {
          "name": "Ground",
          "type": "spinbox"
        },
        {
          "name": "Sub 1",
          "type": "spinbox"
        },
        {
          "type": "spacer",
          "width": 20,
          "col": 3
        },
        {
          "name": "Sub 2 Low",
          "type": "spinbox",
          "row": 2,
          "col": 4
        },
        {
          "name": "Sub 2 High",
          "type": "spinbox",
          "col": 4
        },
        {
          "type": "spacer",
          "height": 40
        },
        {
          "name": "Grid",
          "type": "positions",
          "file": "grid.png",
          "colspan": 5,
          "allowMultiple": true,
          "selectColor": "black"
        },
        {
          "name": "Defense Rating",
          "type": "dropdown",
          "options": [
            "0 (Not Played)",
            "1",
            "2",
            "3",
            "4",
            "5 (Very Effective)"
          ]
        }
      ]
    },
    {
      "name": "Endgame",
      "widgets": [
        {
          "name": "Parking",
          "type": "radio",
          "options": [
            "Docked+Engaged",
            "Docked Only",
            "Parked",
            "None"
          ],
          "default": 3
        },
        {
          "name": "Failed Charging Station",
          "type": "checkbox"
        },
        {
          "name": "Robots Docked",
          "type": "spinbox",
          "max": 3
        },
        {
          "name": "Mechanical Failures",
          "type": "text"
        },
        {
          "name": "Comments",
          "type": "textarea"
        }
      ]
    }
  ]
}
